{
  "name": "ohana-web-search-demo",
  "scripts": {
  },
  "env": {
    "API_KEY": {
      "required": true
    },
    "CANONICAL_URL": {
      "required": true
    },
    "ENABLE_CACHING": {
      "required": true
    },
    "ENABLE_HTTPS": {
      "required": true
    },
    "GOOGLE_ANALYTICS_DOMAIN": {
      "required": true
    },
    "GOOGLE_ANALYTICS_ID": {
      "required": true
    },
    "GOOGLE_MAPS_API_KEY": {
      "required": true
    },
    "LANG": {
      "required": true
    },
    "LATEST_COMMIT_HASH": {
      "required": true
    },
    "MEMCACHIER_PASSWORD": {
      "required": true
    },
    "MEMCACHIER_SERVERS": {
      "required": true
    },
    "MEMCACHIER_USERNAME": {
      "required": true
    },
    "OHANA_API_ENDPOINT": {
      "required": true
    },
    "PATH": {
      "required": true
    },
    "RACK_ENV": {
      "required": true
    },
    "RAILS_ENV": {
      "required": true
    },
    "RAILS_LOG_TO_STDOUT": {
      "required": true,
      "value": "true"
    },
    "RAILS_SERVE_STATIC_FILES": {
      "required": true,
      "value": "true"
    },
    "SECRET_TOKEN": {
      "required": true
    },
    "SENDGRID_PASSWORD": {
      "required": true
    },
    "SENDGRID_USERNAME": {
      "required": true
    },
    "TZ": {
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1,
      "size": "free"
    }
  },
  "addons": [
    "memcachier",
    "sendgrid"
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "heroku/ruby"
    }
  ]
}
